{
  "name": "dogs-classifier",
  "description": "Dogs Classifier",
  "repository": "https://github.com/gpont/dogs-classifier",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --check \"**/*.{css,js,json,md,ts,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{css,js,json,md,ts,yml,yaml}\"",
    "husky:lint-staged": "lint-staged",
    "husky:pre-commit": "run-p husky:lint-staged",
    "lint": "run-p lint:css lint:ts",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "stylelint --fix 'src/**/*.css'",
    "lint:fix": "run-p lint:*:fix",
    "lint:ts": "eslint --resolve-plugins-relative-to \"./node_modules/@ridedott/eslint-config\" \"**/*.ts\"",
    "lint:ts:fix": "eslint --fix --resolve-plugins-relative-to \"./node_modules/@ridedott/eslint-config\" \"**/*.ts\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "test": "jest",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs": "^3.19.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/redux": "^3.6.0",
    "axios": "^0.27.2",
    "identity-obj-proxy": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "serve": "^14.0.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@ridedott/eslint-config": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^25.1.2",
    "@types/webpack-env": "^1.15.1",
    "autoprefixer": "^9.7.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": "v14.18.1",
    "npm": "8.3.0"
  },
  "files": [
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.ts": [
      "eslint --fix --resolve-plugins-relative-to \"./node_modules/@ridedott/eslint-config\""
    ],
    "*.{css,js,json,md,ts,yaml,yml}": [
      "prettier --write"
    ]
  }
}
